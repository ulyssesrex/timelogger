<p id="notice"><%= notice %></p>

<h1>Listing Grantholdings</h1>

<table>
  <thead>
    <tr>
      <th>Grant</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @grantholdings.each do |grantholding| %>
      <tr>
        <td><%= grantholding.grant %></td>
        <td><%= grantholding.user %></td>
        <td><%= link_to 'Show', grantholding %></td>
        <td><%= link_to 'Edit', edit_grantholding_path(grantholding) %></td>
        <td><%= link_to 'Destroy', grantholding, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Grantholding', new_grantholding_path %>
