<% provide(:title, 'New Timelog') %>

<h1>New Timelog</h1>

<%= form_for(@timelog) do |f| %>
	
	<%= render partial: 'form_errors', locals: { timelog: @timelog } %>
	
  <div class="field">
    <%= f.label :start_time  %><br>
    <%= f.text_field :start_time %>
  </div>
  <div class="field">
    <%= f.label :end_time %><br>
    <%= f.text_field :end_time %>
  </div>
  <div class="field">
    <%= f.label :comments %><br>
    <%= f.text_area :comments %>
  </div>  
  <div class="actions">
    <%= f.submit "Cancel" %>
		<%= f.submit "Submit" %>
  </div>
	
<% end %>