<%= form_for @timelog, url: { action: 'update', controller: 'timelogs' } do |f| %>
	
	<%= render partial: 'form_errors', locals: { timelog: @timelog } %>
	
  <div class="field">
    <%= f.label :start_time  %><br>
    <%= f.time_select :start_time %>
  </div>
  <div class="field">
    <%= f.label :end_time %><br>
    <%= f.time_select :end_time %>
  </div>
  <div class="field">
    <%= f.label :comments %><br>
    <%= f.text_area :comments %>
  </div>  
  <div class="actions">
    <%= link_to "Cancel", user_path(@user), class: "btn btn-default" %>
		<%= f.submit "Submit", class: "btn btn-primary" %>
  </div>
	
<% end %>
